<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="seeto.c2.artoria.us.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UI.Signin.SigninActivity">

        </activity>


        <activity android:name=".UI.LeaderBoard.LeaderBoardActivity"></activity>

        <activity android:name=".UI.Main.MainActivity"/>
        <activity android:name=".UI.Store.StoreActivity"/>
        <activity android:name=".UI.Mirror.MirrorActivity"/>
        <activity android:name=".UI.Signup.SignupActivity"/>
        <activity android:name=".UI.Ideas.IdeasViewDetailActivity"
            android:windowSoftInputMode="adjustPan"/>

        <activity android:name=".UI.Ideas.IdeasSelectCategoryActivity"/>

        <activity android:name=".UI.Ideas.IdeasWriteActivity"/>

        <activity android:name=".UI.Setting.SettingActivity"/>

        <activity android:name=".UI.QM.ViewMemoActivity" />

        <activity android:name=".UI.QM.WriteMemoActivity"/>

        <activity android:name=".UI.ToDo.TodoDetailActivity"/>

        <activity android:name=".UI.ToDo.TodoCreate.CreateTodoActivity"
            android:windowSoftInputMode="adjustPan"/>

        <activity android:name=".UI.Splash.Splash"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>